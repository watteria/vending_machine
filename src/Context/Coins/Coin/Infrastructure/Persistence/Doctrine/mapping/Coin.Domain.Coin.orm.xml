<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Context\Coins\Coin\Domain\Coin" table="coins">
        <id name="coin_id" type="coin_id"  column="coin_id" />
        <field name="quantity" type="coin_quantity"  column="quantity" />
        <field name="coin_value" type="coin_value"  column="coin_value" />
        <field name="valid_for_change" type="coin_valid_for_change"  column="valid_for_change" />
    </entity>
</doctrine-mapping>
