<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Context\Customers\Customer\Domain\Customer" table="customers">
        <id name="customer_id" type="customer_id" column="customer_id" />
        <field name="id_product"  type="item_id" column="id_product" />
        <field name="inserted_money" type="customer_inserted_money" column="inserted_money" />
        <field name="status" type="customer_status" column="status" />
    </entity>
</doctrine-mapping>
