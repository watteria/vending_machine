<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Context\Items\Item\Domain\Item" table="items">
        <id name="item_id" type="item_id"  column="item_id" />
        <field name="product_name" type="item_productname" column="product_name" />
        <field name="quantity" type="item_quantity" column="quantity" />
        <field name="price" type="item_price" column="price" />
    </entity>
</doctrine-mapping>
